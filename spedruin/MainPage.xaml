<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="spedruin.MainPage">

   <Grid>

    
<HorizontalStackLayout x:Name="HS1"
VerticalOptions="Start">
    <Image
        Source="fundo.jpg"
         HorizontalOptions="Fill"
         VerticalOptions="Fill"/>
    <Image
        Source="fundo.jpg"
         HorizontalOptions="Fill"
         VerticalOptions="Fill"/>
    <Image
        Source="fundo.jpg"
         HorizontalOptions="Fill"
         VerticalOptions="Fill"/>
</HorizontalStackLayout>

<HorizontalStackLayout x:Name="HS2"
VerticalOptions="Center">
    <Image
        Source="castelo.jpg"
         HorizontalOptions="Fill"
         HeightRequest="200"
         Margin="0,0,0,10"
         VerticalOptions="Start"/>
</HorizontalStackLayout>

<HorizontalStackLayout x:Name="HS3"
VerticalOptions="End">
    <Image
        Source="chao.png"
         HorizontalOptions="Fill"
         VerticalOptions="End"/>
    <Image
        Source="chao.png"
         HorizontalOptions="Fill"
         VerticalOptions="End"/>
    <Image
        Source="chao.png"
         HorizontalOptions="Fill"
         VerticalOptions="End"/>

</HorizontalStackLayout>

    <Image
        Source="corre.png"
        HeightRequest="100"
        HorizontalOptions="Start"
        VerticalOptions="Fill"/>



   </Grid>

</ContentPage>
